{
  "name": "entrepreneur-os",
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "prepare": "husky",
    "nx": "nx",
    "list": "nx list",
    "// === MAKEFILE TARGETS ===": "",
    "make:setup": "nx run entrepreneur-os:make:setup",
    "make:dev": "nx run entrepreneur-os:make:dev",
    "make:docker-up": "nx run entrepreneur-os:make:docker-up",
    "make:docker-down": "nx run entrepreneur-os:make:docker-down",
    "make:test-all": "nx run entrepreneur-os:make:test-all",
    "make:lint-all": "nx run entrepreneur-os:make:lint-all",
    "make:ci": "nx run entrepreneur-os:make:ci",
    "// === DESARROLLO ===": "",
    "dev": "nx run-many -t serve --all --parallel=5",
    "dev:vendure": "nx run-many -t serve --projects=vendure-master,vendure-ecommerce --parallel=2",
    "dev:shops": "nx run-many -t serve --projects=front-master,front-shop-01 --parallel=2",
    "dev:master": "nx serve vendure-master",
    "dev:ecommerce": "nx serve vendure-ecommerce",
    "dev:next": "nx serve storefront-nextjs",
    "dev:vite": "nx serve storefront-vite",
    "dev:front-master": "nx serve front-master",
    "dev:front-shop-01": "nx serve front-shop-01",
    "// === BUILD ===": "",
    "build": "nx run-many -t build --all",
    "build:affected": "nx affected -t build",
    "build:vendure": "nx run-many -t build --projects=vendure-master,vendure-ecommerce",
    "build:shops": "nx run-many -t build --projects=front-master,front-shop-01 --parallel=2",
    "build:front-master": "nx build front-master",
    "build:front-shop-01": "nx build front-shop-01",
    "// === TESTING ===": "",
    "test": "nx run-many -t test --all",
    "test:affected": "nx affected -t test",
    "test:watch": "nx run-many -t test --all --watch",
    "e2e": "nx run-many -t e2e --all",
    "e2e:ui": "playwright test --ui",
    "// === LINTING ===": "",
    "lint": "nx run-many -t lint --all",
    "lint:fix": "nx run-many -t lint --all --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "// === DOCKER ===": "",
    "docker:start": "bash tools/scripts/docker-start.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:stop": "docker-compose stop",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:rebuild": "docker-compose up -d --build --force-recreate",
    "docker:db:reset": "docker-compose down postgres-master postgres-ecommerce && docker volume rm entrepreneur-os_postgres-master-data entrepreneur-os_postgres-ecommerce-data && docker-compose up -d postgres-master postgres-ecommerce",
    "docker:shops": "docker-compose up -d front-master front-shop-01",
    "docker:shops:rebuild": "docker-compose up -d --build front-master front-shop-01",
    "// === DATABASE ===": "",
    "db:reset": "bash tools/scripts/reset-databases.sh",
    "db:seed": "bash tools/scripts/seed-databases.sh",
    "db:migrate": "bash tools/scripts/migrate-databases.sh",
    "// === NX UTILIDADES ===": "",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph",
    "reset": "nx reset",
    "clean": "nx reset && pnpm run docker:clean",
    "// === SETUP ===": "",
    "setup": "pnpm run setup:vendure:master && pnpm run setup:vendure:ecommerce && pnpm run setup:env",
    "setup:vendure:master": "bash tools/scripts/setup-vendure-master.sh",
    "setup:vendure:ecommerce": "bash tools/scripts/setup-vendure-ecommerce.sh",
    "setup:env": "bash tools/scripts/setup-env.sh",
    "// === GENERADORES ===": "",
    "generate": "nx generate",
    "g:lib": "nx generate @nx/js:library",
    "g:app": "nx generate @nx/node:application",
    "g:component": "nx generate @nx/react:component"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@nx/cypress": "^22.0.3",
    "@nx/eslint-plugin": "^22.0.3",
    "@nx/jest": "^22.0.3",
    "@nx/js": "^22.0.3",
    "@nx/next": "^22.0.3",
    "@nx/node": "^22.0.3",
    "@nx/playwright": "^22.0.3",
    "@nx/react": "^22.0.3",
    "@nx/vite": "^22.0.3",
    "@nx/webpack": "^22.0.3",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "9.39.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "{apps,libs,e2e}/**/*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "tools/**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@vitejs/plugin-react": "^5.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gql.tada": "^1.8.13",
    "graphql": "^16.11.0",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.1"
  }
}
